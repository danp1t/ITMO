def password = &quot;password123&quot;

def loginBody = &apos;{&quot;username&quot;:&quot;&apos; + username + &apos;&quot;,&quot;password&quot;:&quot;&apos; + password + &apos;&quot;}&apos;

try {
    def loginSampler = new org.apache.jmeter.protocol.http.sampler.HTTPSamplerProxy()
    loginSampler.setDomain(&quot;localhost&quot;)
    loginSampler.setPort(20041)
    loginSampler.setProtocol(&quot;http&quot;)
    loginSampler.setMethod(&quot;POST&quot;)
    loginSampler.setPath(&quot;/lab1/api/auth/login&quot;)
    loginSampler.addNonEncodedArgument(&quot;&quot;, loginBody, &quot;&quot;)
    loginSampler.setPostBodyRaw(true)
    
    def headers = new org.apache.jmeter.protocol.http.control.HeaderManager()
    headers.add(new Header(&quot;Content-Type&quot;, &quot;application/json&quot;))
    loginSampler.setHeaderManager(headers)
    
    def result = loginSampler.sample(null, null)
    
    if (result.getResponseCode() == &quot;200&quot;) {
        def json = new JsonSlurper().parseText(result.getResponseDataAsString())
        def token = json.token
        log.info(&quot;Поток ${threadNum}: получен токен &quot; + token.substring(0, 30) + &quot;...&quot;)
        vars.put(&quot;auth_token_&quot; + threadNum, token)
    } else {
        log.info(&quot;Поток ${threadNum}: пользователь не найден, регистрируем...&quot;)
        def registerBody = &apos;{&quot;username&quot;:&quot;&apos; + username + &apos;&quot;,&quot;password&quot;:&quot;&apos; + password + &apos;&quot;}&apos;
        
        def registerSampler = new org.apache.jmeter.protocol.http.sampler.HTTPSamplerProxy()
