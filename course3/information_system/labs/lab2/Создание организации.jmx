          <stringProp name="script">import groovy.xml.MarkupBuilder
import java.util.Random
import org.apache.jmeter.services.FileServer

def random = new Random()
def writer = new StringWriter()
def xml = new MarkupBuilder(writer)

xml.organizations {
    organization {
        name &quot;Компания_${System.currentTimeMillis()}_${random.nextInt(1000)}&quot;
        annualTurnover &quot;1500000.50&quot;
        employeesCount &quot;250&quot;
        rating &quot;8&quot;
        type &quot;COMMERCIAL&quot;
        coordinates {
            x &quot;10.5&quot;
            y &quot;20.75&quot;
        }
        officialAddress {
            x &quot;55.7558&quot;
            y &quot;37.6173&quot;
            z &quot;-6&quot;
            name &quot;Москва, Кремль&quot;
        }
        postalAddress {
            street &quot;ул. Тверская&quot;
            zipCode &quot;101000&quot;
        }
    }
}

def tempDir = System.getProperty(&quot;java.io.tmpdir&quot;)
def fileName = &quot;temp_import_${System.currentTimeMillis()}.xml&quot;
def tempFile = new File(tempDir, fileName)
tempFile.write(writer.toString(), &quot;UTF-8&quot;)

vars.put(&quot;tempFilePath&quot;, tempFile.getAbsolutePath())

log.info(&quot;Временный файл создан: &quot; + tempFile.getAbsolutePath())

import org.apache.jmeter.protocol.http.control.Header
import groovy.json.JsonSlurper

def threadNum = ctx.getThreadNum()
def threadId = ctx.getThreadGroup().getName() + &quot;_&quot; + threadNum

def username = &quot;jmeter_thread_&quot; + threadNum
def password = &quot;password123&quot;

def loginBody = &apos;{&quot;username&quot;:&quot;&apos; + username + &apos;&quot;,&quot;password&quot;:&quot;&apos; + password + &apos;&quot;}&apos;

try {
    def loginSampler = new org.apache.jmeter.protocol.http.sampler.HTTPSamplerProxy()
    loginSampler.setDomain(&quot;localhost&quot;)
    loginSampler.setPort(20041)
    loginSampler.setProtocol(&quot;http&quot;)
    loginSampler.setMethod(&quot;POST&quot;)
    loginSampler.setPath(&quot;/lab1/api/auth/login&quot;)
    loginSampler.addNonEncodedArgument(&quot;&quot;, loginBody, &quot;&quot;)
    loginSampler.setPostBodyRaw(true)
    
    def headers = new org.apache.jmeter.protocol.http.control.HeaderManager()
    headers.add(new Header(&quot;Content-Type&quot;, &quot;application/json&quot;))
    loginSampler.setHeaderManager(headers)
    
    def result = loginSampler.sample(null, null)
    
    if (result.getResponseCode() == &quot;200&quot;) {
        def json = new JsonSlurper().parseText(result.getResponseDataAsString())
        def token = json.token
        log.info(&quot;Поток ${threadNum}: получен токен &quot; + token.substring(0, 30) + &quot;...&quot;)
        vars.put(&quot;auth_token_&quot; + threadNum, token)
    } else {
        log.info(&quot;Поток ${threadNum}: пользователь не найден, регистрируем...&quot;)
        def registerBody = &apos;{&quot;username&quot;:&quot;&apos; + username + &apos;&quot;,&quot;password&quot;:&quot;&apos; + password + &apos;&quot;}&apos;
        
        def registerSampler = new org.apache.jmeter.protocol.http.sampler.HTTPSamplerProxy()
        registerSampler.setDomain(&quot;localhost&quot;)
        registerSampler.setPort(20041)
        registerSampler.setProtocol(&quot;http&quot;)
        registerSampler.setMethod(&quot;POST&quot;)
        registerSampler.setPath(&quot;/lab1/api/auth/register&quot;)
        registerSampler.addNonEncodedArgument(&quot;&quot;, registerBody, &quot;&quot;)
        registerSampler.setPostBodyRaw(true)
        registerSampler.setHeaderManager(headers)
        
        def registerResult = registerSampler.sample(null, null)
        
        if (registerResult.getResponseCode() == &quot;200&quot;) {
            def json = new JsonSlurper().parseText(registerResult.getResponseDataAsString())
            def token = json.token
            log.info(&quot;Поток ${threadNum}: зарегистрирован, токен &quot; + token.substring(0, 30) + &quot;...&quot;)
            vars.put(&quot;auth_token_&quot; + threadNum, token)
        } else {
            log.error(&quot;Поток ${threadNum}: ошибка регистрации: &quot; + registerResult.getResponseDataAsString())
        }
    }
} catch (Exception e) {
    log.error(&quot;Поток ${threadNum}: ошибка: &quot; + e.getMessage())
}
def token = vars.get(&quot;auth_token_&quot; + threadNum)

if (token) {
    def headerManager = sampler.getHeaderManager()
    if (headerManager == null) {
        headerManager = new org.apache.jmeter.protocol.http.control.HeaderManager()
        sampler.setHeaderManager(headerManager)
    }
    
    headerManager.getHeaders().removeIf { it.getName().equalsIgnoreCase(&quot;Authorization&quot;) }
    
    headerManager.add(new Header(&quot;Authorization&quot;, &quot;Bearer &quot; + token))
    log.info(&quot;Поток ${threadNum}: установлен заголовок Authorization&quot;)
} else {
    log.error(&quot;Поток ${threadNum}: не удалось получить токен&quot;)
}</stringProp>
