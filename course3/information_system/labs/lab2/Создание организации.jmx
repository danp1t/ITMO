        registerSampler.setPort(20041)
        registerSampler.setProtocol(&quot;http&quot;)
        registerSampler.setMethod(&quot;POST&quot;)
        registerSampler.setPath(&quot;/lab1/api/auth/register&quot;)
        registerSampler.addNonEncodedArgument(&quot;&quot;, registerBody, &quot;&quot;)
        registerSampler.setPostBodyRaw(true)
        registerSampler.setHeaderManager(headers)
        
        def registerResult = registerSampler.sample(null, null)
        
        if (registerResult.getResponseCode() == &quot;200&quot;) {
            def json = new JsonSlurper().parseText(registerResult.getResponseDataAsString())
            def token = json.token
            log.info(&quot;Поток ${threadNum}: зарегистрирован, токен &quot; + token.substring(0, 30) + &quot;...&quot;)
            vars.put(&quot;auth_token_&quot; + threadNum, token)
        } else {
            log.error(&quot;Поток ${threadNum}: ошибка регистрации: &quot; + registerResult.getResponseDataAsString())
        }
    }
} catch (Exception e) {
